msgctxt "#30000"
msgid "Server Hostname or IP"
msgstr "Nafn miðlara eða IP-tala"

msgctxt "#30001"
msgid "Keep IP address to 127.0.0.1, if server is running on this same system"
msgstr "Hafðu IP-töluna 127.0.0.1, ef miðlari er á keyra á sömu tölvu"

msgctxt "#30002"
msgid "Port Number"
msgstr "Gátt númer"

msgctxt "#30003"
msgid "Stream timeout, unknown problem"
msgstr "Streymi rann út á tíma, óþekkt vandamál"

msgctxt "#30004"
msgid "No video data found on this channel"
msgstr "Engin myndbandsgögn fundust á þessari rás"

msgctxt "#30005"
msgid "Tuner is needed for recording"
msgstr "Þarf móttakara fyrir upptöku"

msgctxt "#30006"
msgid "Recording delete failed, file in use or write protected"
