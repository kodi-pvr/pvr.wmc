msgid "Anytime"
msgstr "Jebkurā laikā"

msgctxt "#30120"
msgid "Delete Timer"
msgstr "Dzēst taimeri"

msgctxt "#30121"
msgid "Delete this episode timer only"
msgstr "Dzēst tikai šo sērijas taimeri"

msgctxt "#30122"
msgid "Delete the entire series"
msgstr "Dzēst visas sērijas"

