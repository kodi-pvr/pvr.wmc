msgctxt "#30000"
msgid "Server Hostname or IP"
msgstr "Hostname u IP tas-server"

msgctxt "#30001"
msgid "Keep IP address to 127.0.0.1, if server is running on this same system"
msgstr "Żomm l-indirizz tal-IP 127.0.0.1, jekk is-server qed jaħdem fuq din i-sistema stess"

